extends ../../templates/page/template.page-base-adaptativo.pug

block styles
	link(rel="stylesheet", href="style.css")

block body
	.body.body01
		+header()

		.fundo
			+image('capa.png', 'capa')
			
			.titleBase.zoomIn.delay2  
				.title
					+image('title.svg', '')

		
			section.sec1
				.form.up.delay4
					.input-wrapper
						label(for="nome") 
							p NOME 
						input#name(type="text")
						small.alertName Preencha o nome. 
					.input-wrapper
						label(for="email") 
							p E-MAIL CORPORATIVO 
						input#email(type="mail")
						small.alertEmail E-mail invalido.
				.boxBlack.up.delay5
					
					h2 PREPARE-SE PARA RESPONDER AS QUESTÃ•ES NO MENOR TEMPO!

					button.btnInit.cadastro
						+image('iniciarBtn.svg', '')
					
		
block scripts
	script(src="script.js")